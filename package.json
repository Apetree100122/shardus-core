{
  "name": "shardus-enterprise-server",
  "version": "0.0.0",
  "description": "The shardus enterprise server",
  "main": "server.js",
  "scripts": {
    "test": "standard && tap \"test/**/*.spec.js\"",
    "test:watch": "nodemon -x npm run test",
    "test:unit": "tap \"test/unit/**/*.spec.js\"",
    "test:unit:watch": "nodemon -x npm run test:unit",
    "test:integration": "tap \"test/integration/**/*.spec.js\"",
    "test:integration:watch": "nodemon -x npm run test:integration",
    "cover": "nyc tap \"test/**/*.spec.js\" --coverage-report=html && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:shardus/enterprise/shardus-enterprise-server.git"
  },
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "log4js": "3.0.6",
    "sequelize": "4.41.0",
    "shardus-crypto-utils": "gitlab:shardus/shardus-crypto-utils",
    "shardus-quic-net": "gitlab:shardus/shardus-quic-net",
    "sqlite": "3.0.0"
  },
  "devDependencies": {
    "axios": "0.18.0",
    "codecov": "3.1.0",
    "nodemon": "1.18.4",
    "nyc": "13.1.0",
    "standard": "12.0.1",
    "tap": "12.0.1"
  }
}
